#        注意:出于逻辑原因，不支持角色禁止。
#

PacketSpoof.BanMode = 0

#
#    PacketSpoof.BanDuration
#        Description:禁令的持续时间（以秒为单位）。仅在PacketSpoof.Policy设置为2时有效。
#        设置为0表示永久禁止。
#        Default:     86400 seconds (1 天)
#

PacketSpoof.BanDuration = 86400

