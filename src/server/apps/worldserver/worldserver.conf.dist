# PACKET SPOOF PROTECTION SETTINGS-数据包欺骗保护设置
#
# 这些设置决定在检测到有害数据包欺骗时采取何种措施。
#
#    PacketSpoof.Policy
#        说明：确定检测到数据包欺骗时的处理过程。
#        Values:     0 - 仅记录
#                    1 - 记录+踢掉
#                    2 - 记录+踢掉+禁止

PacketSpoof.Policy = 1

#
#    PacketSpoof.BanMode
#        说明：如果PacketSpoof.Policy等于2，则将确定禁止模式。
#        Values:     0 - Ban Account
#                    1 - Ban IP
#        注意:出于逻辑原因，不支持角色禁止。
#

PacketSpoof.BanMode = 0

#
#    PacketSpoof.BanDuration
#        Description:禁令的持续时间（以秒为单位）。仅在PacketSpoof.Policy设置为2时有效。
#        设置为0表示永久禁止。
#        Default:     86400 seconds (1 天)
#

PacketSpoof.BanDuration = 86400

