################################################
# AzerothCore World Server configuration file #
################################################
[worldserver]

###################################################################################################
# SECTION INDEX-快速索引
#
#    EXAMPLE CONFIG
#    CONNECTIONS AND DIRECTORIES
#    PERFORMANCE SETTINGS
#    SERVER LOGGING
#    SERVER SETTINGS
#    CRYPTOGRAPHY
#    WARDEN SETTINGS
#    PLAYER INTERACTION
#    CREATURE SETTINGS
#    CHAT SETTINGS
#    GAME MASTER SETTINGS
#    VISIBILITY AND DISTANCES
#    SERVER RATES
#    STATS LIMITS
#    AUTO BROADCAST
#    BATTLEGROUND CONFIG
#    BATTLEFIELD CONFIG
#    ARENA CONFIG
#    NETWORK CONFIG
#    CONSOLE AND REMOTE ACCESS
#    CHARACTER DELETE OPTIONS
#    ITEM DELETE OPTIONS
#    CUSTOM SERVER OPTIONS
#    UPDATE SETTINGS
#    LOGGING SYSTEM SETTINGS
#    PACKET SPOOF PROTECTION SETTINGS
#    DEBUG
#    METRIC SETTINGS
#
###################################################################################################

###################################################################################################
# EXAMPLE CONFIG-示例配置
#
#    Variable
#        Description:简要描述变量正在做什么。
#        重要：有关此变量的重要事项的注释。
#        Example:     "Example, i.e. if the value is a string"
#        Default:     10 - (Enabled|Comment|Variable name in case of grouped config options)
#                     0  - (Disabled|Comment|Variable name in case of grouped config options)
#
# Note to developers:
# - Copy this example to keep the formatting.
# - Line breaks should be at column 100.
###################################################################################################

###################################################################################################
# CONNECTIONS AND DIRECTORIES
#
#    RealmID
#        Description:使用此配置的RealmdID。
#        重要提示:RealmID必须与auth数据库中的realmlist匹配。
#        Default:     1

RealmID = 1

#
#    DataDir
#        Description: 数据目录设置。
#        Important:   DataDir需要加引号，因为字符串可能包含空格字符。
#        Example:     "@prefix@\home\youruser\azerothcore\data"
#        Default:     "."

DataDir = "/home/server/data"

#
#    LogsDir
#        Description: 日志目录设置。
#        Important:   LogsDir需要加引号，因为字符串可能包含空格字符。
#                     日志目录必须存在，否则将无法创建日志文件。
#        Example:     "/home/youruser/azerothcore/logs"
#        Default:     "" - (日志文件将保存在当前路径下)

LogsDir = "/home/server/log"

#
#    LoginDatabaseInfo
#    WorldDatabaseInfo
#    CharacterDatabaseInfo
#        Description: 世界服务器的数据库连接设置。
#        Example:     "hostname;port;username;password;database"
#                     ".;somenumber;username;password;database" - (Use named pipes on Windows
#                                                                 "enable-named-pipe" to [mysqld]
#                                                                 section my.ini)
#                     ".;/path/to/unix_socket;username;password;database" - (use Unix sockets on
#                                                                           Unix/Linux)
#        Default:     "127.0.0.1;3306;acore;acore;acore_auth"       - (LoginDatabaseInfo)
#                     "127.0.0.1;3306;acore;acore;acore_world"      - (WorldDatabaseInfo)
#                     "127.0.0.1;3306;acore;acore;acore_characters" - (CharacterDatabaseInfo)

LoginDatabaseInfo     = "172.16.0.9;3306;root;A111111a!;auth"
WorldDatabaseInfo     = "172.16.0.9;3306;root;A111111a!;world"
CharacterDatabaseInfo = "172.16.0.9;3306;root;A111111a!;characters"

#
#    Database.Reconnect.Seconds
#    Database.Reconnect.Attempts
#
#        Description: 每次重新连接的间隔时间是多少秒
#                     以及总共要进行多少次尝试
#        Default:    每15秒有20次尝试
#

Database.Reconnect.Seconds = 15
Database.Reconnect.Attempts = 20

#
#    LoginDatabase.WorkerThreads
#    WorldDatabase.WorkerThreads
#    CharacterDatabase.WorkerThreads
#        Description: 用于处理异步(延迟)MySQL语句的工作线程数量。
#                     每个工作线程都有自己到MySQL服务器的连接。
#                     和自己在MySQL服务器上的线程。
#        Default:     1 - (LoginDatabase.WorkerThreads)
#                     1 - (WorldDatabase.WorkerThreads)
#                     1 - (CharacterDatabase.WorkerThreads)

LoginDatabase.WorkerThreads     = 1
WorldDatabase.WorkerThreads     = 1
CharacterDatabase.WorkerThreads = 1

#
#    LoginDatabase.SynchThreads
#    WorldDatabase.SynchThreads
#    CharacterDatabase.SynchThreads
#        Description: 需要处理的MySQL连接数量。
#        Default:     1 - (LoginDatabase.WorkerThreads)
#                     1 - (WorldDatabase.WorkerThreads)
#                     2 - (CharacterDatabase.WorkerThreads)

LoginDatabase.SynchThreads     = 1
WorldDatabase.SynchThreads     = 1
CharacterDatabase.SynchThreads = 2

#
#    MaxPingTime
#        Description: 数据库ping之间的时间(分钟)。
#        Default:     30

MaxPingTime = 30

#
#    WorldServerPort
#        Description: 连接world服务器的TCP端口。
#        Default:     8085

WorldServerPort = 8085

#
#    BindIP
#        Description: Bind world server to IP/hostname
#        Default:     "0.0.0.0" - (Bind to all IPs on the system)

BindIP = "0.0.0.0"

#
#    CMakeCommand
#        Description: CMake二进制文件的路径。
#                     如果路径为空，则使用内置的CMAKE_COMMAND命令。
#        Example:     "C:/Program Files/CMake/bin/cmake.exe"
#                     "/usr/bin/cmake"
#        Default:     ""

CMakeCommand = ""

#
#    BuildDirectory
#        Description: 生成目录的路径。
#                     如果路径为空，则使用内置的CMAKE_BINARY_DIR。
#        Example:     "../AzerothCore"
#        Default:     ""

BuildDirectory = "/home/server/"

#
#    SourceDirectory
#        Description: AzerothCore源目录的路径。
#                     如果路径为空，则使用内置的CMAKE_SOURCE_DIR。
#        Example:     "../AzerothCore"
#        Default:     ""

SourceDirectory = "/home/azerothcore-wotlk"

#
#    MySQLExecutable
#        Description: MySQL CLI二进制文件的路径。
#                     如果该路径为空，则使用cmake的内置路径。
#        Example:     "C:/Program Files/MySQL/MySQL Server 5.7/bin/mysql.exe"
#                     "mysql.exe"
#                     "/usr/bin/mysql"
#        Default:     ""

MySQLExecutable = ""

#
#    ThreadPool
#        Description: 用于全局线程池的线程数
#                     线程池目前用于:
#                      - Signal handling-信号处理
#                      - Remote access-远程访问
#                      - Database keep-alive ping
#                      - Core freeze check-核心冻结检查
#                      - World socket networking
#        Default:     2

ThreadPool = 2

#
#    IPLocationFile
#        Description: IP2Location数据库CSV文件的路径。
#        Example:     "C:/acore/IP2LOCATION-LITE-DB1.CSV"
#                     "/home/acore/IP2LOCATION-LITE-DB1.CSV"
#        Default:     ""  - (Disabled)
#

IPLocationFile = ""

#
#    AllowLoggingIPAddressesInDatabase
#        Description: 指定是否可以将IP地址记录到数据库中
#        Default:     1 - (Enabled)
#                     0 - (Disabled)
#

AllowLoggingIPAddressesInDatabase = 1

#
###################################################################################################

###################################################################################################
# PERFORMANCE SETTINGS-性能设置
#
#    UseProcessors
#        Description:基于Windows和Linux的多处理器系统的处理器掩码。
#        示例:对于一台有3个cpu的计算机:
#                     1 - 1st CPU only
#                     2 - 2nd CPU only
#                     4 - 3rd CPU only
#                     6 - 2nd + 3rd CPUs, because "2 | 4" -> 6
#        Default:     0  - (由操作系统选择)
#                     1+ - (Bit mask value of selected processors)

UseProcessors = 0

#
#    ProcessPriority
#        Description:Windows系统的进程优先级设置。
#        Default:     1 - (High)
#                     0 - (Normal)

ProcessPriority = 1

#
#    Compression
#        Description:客户端更新包的压缩级别
#        Range:       1-9
#        Default:     1   - (速度)
#                     9   - (最佳压缩)

Compression = 1

#
#    PlayerLimit
#        Description:world上最大的玩家数量。不包括mod, GMs和管理员。
#        重要提示：如果您想阻止玩家，并且仅允许Mod，GM或Admins加入服务器，
#        使用数据库字段 "auth.realmlist.allowedSecurityLevel".
#            Default: 1000 - (Enabled)
#                     1+  - (Enabled)
#                     0   - (禁用，无限制)

PlayerLimit = 0

#
#    SaveRespawnTimeImmediately
#        说明：保存死亡生物和使用/打开时游戏物体的重生时间。
#        Default:     1 - (启用，立即保存重生时间)
#                     0 - (Disabled, 节省网格卸载时的重生时间)

SaveRespawnTimeImmediately = 1

#
#    MaxOverspeedPings
#        Description: Maximum overspeed ping count before character is disconnected.
#        Default:     2  - (Enabled, Minimum value)
#                     3+ - (Enabled, More checks before kick)
#                     0  - (Disabled)

MaxOverspeedPings = 2

#
#    CloseIdleConnections
#        Description:自动关闭空闲连接。
#        SocketTimeOutTime和SocketTimeOutTimeActive决定连接何时被认为是空闲的。
#        Default:     1 - (启用，自动关闭空闲连接)
#                     0 - (disable, 不关闭空闲连接)

CloseIdleConnections = 1

#
#    SocketTimeOutTime
#        Description: 角色连接空闲的时间(以毫秒为单位)
#                     选择屏幕断开。
#        Default:     900000 - (15分钟)

SocketTimeOutTime = 900000

#
#    SocketTimeOutTimeActive
#        Description: 空闲连接被丢弃的时间(以毫秒为单位)
#                     登录到世界。
#                     客户端每30秒发送一次keepalive报文。不建议设置<= 30s。
#        Default:     60000 - (1分钟)

SocketTimeOutTimeActive = 60000

#
#    SessionAddDelay
#        Description: 网络线程在验证后休眠的时间(以微秒为单位)
#                     将连接添加到世界会话map之前的协议处理。
#        Default:     10000 - (10毫秒0.01秒)

SessionAddDelay = 10000

#
#    MapUpdateInterval
#        Description:地图更新间隔的时间(毫秒)。
#        Default:     100 - (0.1 秒)

MapUpdateInterval = 100

#
#    ChangeWeatherInterval
#        Description:天气更新间隔的时间(毫秒)。
#        Default:     600000 - (10 分钟)

ChangeWeatherInterval = 600000

#
#    PlayerSaveInterval
#        Description:玩家保存间隔的时间(以毫秒为单位)。
#        Default:     900000 - (15 分钟)

PlayerSaveInterval = 300000

#
#    PlayerSave.Stats.MinLevel
#        说明：在数据库中保存角色统计信息以供外部使用的最低级别。
#        Default:     0  - (禁用，不保存角色统计信息)
#                     1+ - (启用，超过该级别的角色属性将被保存)

PlayerSave.Stats.MinLevel = 1

#
#    PlayerSave.Stats.SaveOnlyOnLogout
#        Description: 只在登出时保存玩家状态。
#        Default:     1 - (Enabled, 仅在注销时保存)
#                     0 - (Disabled, 保存每一个玩家状态)

PlayerSave.Stats.SaveOnlyOnLogout = 0

#
#    vmap.enableLOS
#    vmap.enableHeight
#        Description: VMmap支持视线和高度计算。
#        Default:     1 - (Enabled, vmap.enableLOS)
#                     1 - (Enabled, vmap.enableHeight)
#                     0 - (Disabled)

vmap.enableLOS    = 1
vmap.enableHeight = 1

#
#    vmap.petLOS
#        Description: 检查宠物的视线，避免它们通过墙壁攻击。
#        Default:     1 - (Enabled, 宠物的每一次攻击都将根据视线进行检查)
#                     0 - (Disabled, 略微减少CPU使用)

vmap.petLOS = 1

#    vmap.BlizzlikePvPLOS
#        Description: 检查战场和竞技场游戏对象和其他小玩意儿（如 WSG 树桩）的视线。
#        Default:     1 - (Enabled, 玩家将能够通过树桩和其他物体发射法术).
#                     0 - (Disabled, 玩家将无法通过树桩和其他物体发射法术).

vmap.BlizzlikePvPLOS = 1

#
#    vmap.enableIndoorCheck
#        Description: 基于VMap的室内检查来移除室外的光环(坐骑等)。
#        Default:     1 - (Enabled)
#                     0 - (Disabled, somewhat less CPU usage)

vmap.enableIndoorCheck = 1

#
#    DetectPosCollision
#        Description: 检查最终移动位置、召唤位置等与其他物体或墙壁的可见碰撞
#                     (仅当vmaps启用时墙壁)。
#        Default:     1 - (Enabled)
#                     0 - (Disabled, 位置精度降低，但CPU占用率降低)

DetectPosCollision = 1

#
#    CheckGameObjectLoS
#        Description: 包括动态游戏对象(门，箱子等)在视线检查。
#                     这在一定程度上增加了CPU的使用。
#        Default:     1 - (Enabled)
#                     0 - (Disabled, may break some boss encounters)

CheckGameObjectLoS = 1

#
#    TargetPosRecalculateRange
#        Description: 距离移动目标点(+移动单位大小)和目标的最大距离
#                     对象(+大小)后，新的目标移动点计算。
#        Range:       0.5-5.0
#        Default:     1.5
#                     0.5 - (最小，接触距离，对目标移动更敏感)
#                     5.0 - (最大，近战攻击距离，更少的CPU占用)

TargetPosRecalculateRange = 1.5

#
#    UpdateUptimeInterval
#        说明：更新领域正常运行时间（以分钟为单位）。
#        Default:     10 - (10分钟)
#                     1+

UpdateUptimeInterval = 10

#
#    LogDB.Opt.ClearInterval
#        Description: WUPDATE_CLEANDB定时器清除旧条目的' logs '表的时间
#                     (以分钟为单位)。
#        Default:     10 - (10分钟)
#                     1+

LogDB.Opt.ClearInterval = 10

#
#    LogDB.Opt.ClearTime
#        Description: 保留旧`log`表条目的时间（以秒为单位）。
#        Default:     1209600 - (Enabled, 14 天)
#                     0       - (Disabled, 不清除条目)

LogDB.Opt.ClearTime = 1209600

#
#    MaxCoreStuckTime
#        Description: 如果服务器被冻结，它被迫崩溃之前的时间(以秒为单位)。
#        Default:     0   - (Disabled)
#                     10+ - (Enabled, 推荐 30+)
# 注意：如果启用并且设置太低，则可能导致意外崩溃。

MaxCoreStuckTime = 0

#
#    AddonChannel
#        Description:通过服务器配置插件通道的使用
#                     (禁用插件通道后，一些客户端插件将无法正常工作)
#        Default:     1 - (Enabled)
#                     0 - (Disabled)

AddonChannel = 1

#
#    MapUpdate.Threads
#        Description:更新maps的线程数。
#        Default:     1

MapUpdate.Threads = 1

#
#    CleanCharacterDB
#        Description: 清除数据库中已废弃的成就，技能，法术和天赋。
#        Default:     0 - (Disabled)
#                     1 - (Enable)

CleanCharacterDB = 0

#
#    PersistentCharacterCleanFlags
#        Description: 确定清除后保持设置的角色清除标志。
#                     这是一个位掩码值，你可以使用以下值之一:
#
#                     CLEANING_FLAG_ACHIEVEMENT_PROGRESS  = 0x1
#                     CLEANING_FLAG_SKILLS                = 0x2
#                     CLEANING_FLAG_SPELLS                = 0x4
#                     CLEANING_FLAG_TALENTS               = 0x8
#                     CLEANING_FLAG_QUESTSTATUS           = 0x10
#
#                     在使用此功能之前，请对数据库进行备份。
#
#        Example:     14 - (CLEANING_FLAG_SKILLS + CLEANING_FLAG_SPELLS + CLEANING_FLAG_TALENTS
#                           2+4+8 => 14. 这将清理技能,天赋和法术
#                           在下一次清除后保持启用状态)
#        Default:     0  - (在下一次清除之后，所有的清除方法将被禁用)

PersistentCharacterCleanFlags = 0

#
#    PreloadAllNonInstancedMapGrids
#        Description:在所有非副本地图上预加载所有网格。
#                     这将需要大量额外的RAM(约9 GB)，并导致服务器需要更长的启动时间，
#                     但如果在有大量玩家的服务器上使用，则可以提高性能。
#                     它也会在服务器启动时激活所有被设置为激活的生物
#                     (例如地狱火半岛的恶魔掠夺者)。
#        Default:     0 - (Disabled)
#                     1 - (Enabled)

PreloadAllNonInstancedMapGrids = 0

#
#    SetAllCreaturesWithWaypointMovementActive
#        说明：设置所有移动路径点的生物。
#                     这意味着它们将在加载后开始移动(这发生在网格加载时)，
#                     并在没有玩家靠近时继续移动。这将显著增加CPU使用量，
#                     并可与启用的“PreloadAllNonInstancedMapGrids”一起使用，
#                     在服务器启动时启动waypoint移动。
#        Default:     0 - (Disabled)
#                     1 - (Enabled)

SetAllCreaturesWithWaypointMovementActive = 0

#
###################################################################################################

###################################################################################################
# SERVER LOGGING-服务器日志
#
#    PidFile
#        Description: 世界守护进程PID文件。
#        Example:     "./world.pid" - (Enabled)
#        Default:     ""            - (Disabled)

PidFile = "./world.pid"

#
#    PacketLogFile
#        Description: 世界服务器的二进制包日志文件。
#                     文件名扩展名必须是.pkt才能被WowPacketParser解析。
#        Example:     "World.pkt" - (Enabled)
#        Default:     ""          - (Disabled)

PacketLogFile = ""

# 扩展日志记录系统配置移到文件末尾(有意的)
#
###################################################################################################

###################################################################################################
# SERVER SETTINGS-服务器设置
#
#    GameType
#        Description:服务器类型。
#        Default:     0  - (NORMAL)
#                     1  - (PVP)
#                     4  - (NORMAL)
#                     6  - (RP)
#                     8  - (RPPVP)
#                     16 - (FFA_PVP, Free for all PvP mode like arena PvP in all zones except rest
#                          activated places and sanctuaries)

GameType = 1

#
#    RealmZone
#        Description: 服务器领域区。设置允许使用的字母、字符等名称。
#        Default      1  - (Development   - 任何语言)
#                     2  - (美国 - 扩展-拉丁文)
#                     3  - (Oceanic       - 扩展-拉丁文)
#                     4  - (拉丁美洲 - 扩展-拉丁文)
#                     5  - (Tournament    - basic-Latin at create, any at login)
#                     6  - (韩国         - 东方-亚洲)
#                     7  - (Tournament    - basic-Latin at create, any at login)
#                     8  - (英国       - 扩展-拉丁文)
#                     9  - (德国        - 扩展-拉丁文)
#                     10 - (法国        - 扩展-拉丁文)
#                     11 - (西班牙       - 扩展-拉丁文)
#                     12 - (俄罗斯       - Cyrillic)
#                     13 - (Tournament    - basic-Latin at create, any at login)
#                     14 - (台湾        - 东方-亚洲)
#                     15 - (Tournament    - basic-Latin at create, any at login)
#                     16 - (中国         - 东方-亚洲)
#                     17 - (CN1           - basic-Latin at create, any at login)
#                     18 - (CN2           - basic-Latin at create, any at login)
#                     19 - (CN3           - basic-Latin at create, any at login)
#                     20 - (CN4           - basic-Latin at create, any at login)
#                     21 - (CN5           - basic-Latin at create, any at login)
#                     22 - (CN6           - basic-Latin at create, any at login)
#                     23 - (CN7           - basic-Latin at create, any at login)
#                     24 - (CN8           - basic-Latin at create, any at login)
#                     25 - (Tournament    - basic-Latin at create, any at login)
#                     26 - (测试服务器   - 任何语言)
#                     27 - (Tournament    - basic-Latin at create, any at login)
#                     28 - (QA Server     - 任何语言)
#                     29 - (CN9           - basic-Latin at create, any at login)

RealmZone = 16

#
#    World.RealmAvailability
#        Description: 如果启用，玩家将正常进入服务器。
#                     如果禁用，只可以创建角色。
#        Default:     1 - (Enabled)
#                     0 - (Disabled)

World.RealmAvailability = 1

#
#    StrictPlayerNames
#        Description: 将玩家名限制为特定语言的符号集。
#                     如果使用了不允许的符号，则防止创建字符并强制重命名请求
#        Default:     0 - (Disable, 有限的服务器时区依赖客户端检查)
#                     1 - (Enabled, 严格的基本拉丁字符)
#                     2 - (Enabled, 严格限定于领域区域，请参阅RealmZone设置，
#                         注释: 客户端需要安装支持字符集的适当字体。
#                         非官方的本地化,
#                         自定义字体需要放置在clientdir/Fonts.
#                     3 - (Enabled, 基本拉丁字符 + 服务器时区特定)

StrictPlayerNames = 0

#
#    StrictCharterNames
#        Description: 限制公会/竞技场团队名称为语言特定的符号集。
#                     如果使用不允许的符号，则阻止创建。
#        Default:     0 - (Disable, Limited server timezone dependent client check)
#                     1 - (Enabled, Strictly basic Latin characters)
#                     2 - (Enabled, Strictly realm zone specific, See RealmZone setting,
#                         Note: Client needs to have the appropriate fonts installed which support
#                         the charset. For non-official localization, custom fonts need to be
#                         placed in clientdir/Fonts.
#                     3 - (Enabled, Basic Latin characters + server timezone specific)

StrictCharterNames = 3

#
#    StrictPetNames
#        Description: 限制宠物名称为语言特定的符号集。
#                     果使用了不允许的符号，可以防止宠物命名。
#        Default:     0 - (Disable, Limited server timezone dependent client check)
#                     1 - (Enabled, Strictly basic Latin characters)
#                     2 - (Enabled, Strictly realm zone specific, See RealmZone setting,
#                         Note: Client needs to have the appropriate fonts installed which support
#                         the charset. For non-official localization, custom fonts need to be
#                         placed in clientdir/Fonts.
#                     3 - (Enabled, Basic Latin characters + server timezone specific)

StrictPetNames = 3

#
#    DBC.Locale
#        Description: DBC语言设置。
#        Default:     255 - (Auto Detect-自动检测)
#                     0   - (English-英语)
#                     1   - (Korean-韩语)
#                     2   - (French-法语)
#                     3   - (German-德语)
#                     4   - (Chinese-汉语)
